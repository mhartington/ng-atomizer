{
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "name": "test-atomizer",
  "projectType": "application",
  "generators": {},
  "sourceRoot": "src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "options": {
        "outputPath": "dist/test-atomizer",
        "index": "src/index.html",
        "browser": "src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "public"
          }
        ],
        "styles": [
          "src/styles.css"
        ],
        "scripts": [],
        "server": "src/main.server.ts",
        "outputMode": "server",
        "ssr": {
          "entry": "src/server.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kB",
              "maximumError": "1MB"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kB",
              "maximumError": "8kB"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "test-atomizer:build:production"
        },
        "development": {
          "buildTarget": "test-atomizer:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "e2e": {
      "executor": "playwright-ng-schematics:playwright",
      "options": {
        "devServerTarget": "test-atomizer:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "test-atomizer:serve:production"
        }
      }
    }
  }
}